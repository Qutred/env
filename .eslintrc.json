{
  "extends": "airbnb-base",
  "env": {
    "es2022": true,
    "browser": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "rules": {
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
